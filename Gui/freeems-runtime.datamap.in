[global]
description=Object data map for MegaSquirt Runtime Display
_tab_name=_Runtime Display
groups=defaults
post_functions=load_rt_sliders

#----- Groups ----------------------------------------------------------------

[defaults]
keys=page,size
page=0
size=_U08_

#----- Buttons ---------------------------------------------------------------

#------ Gui objects ---------------------------------------------------------

[topframe]
keys=tab_ident
tab_ident=_RUNTIME_TAB_

#------- Runtime Status Indicators ------------------------------------------

[runtime_crank_sync_label]
keys=bind_to_list,source,bitval,bitmask,active_fg,inactive_fg
active_fg=red
inactive_fg=dim grey
bind_to_list=runtime_status
source=decoderFlags
bitval=1
bitmask=1

[runtime_combustion_sync_label]
keys=bind_to_list,source,bitval,bitmask,active_fg,inactive_fg
active_fg=red
inactive_fg=dim grey
bind_to_list=runtime_status
source=decoderFlags
bitval=1
bitmask=2

[runtime_cam_sync_label]
keys=bind_to_list,source,bitval,bitmask,active_fg,inactive_fg
active_fg=red
inactive_fg=dim grey
bind_to_list=runtime_status
source=decoderFlags
bitval=1
bitmask=4

[runtime_last_stamp_valid_label]
keys=bind_to_list,source,bitval,bitmask,active_fg,inactive_fg
active_fg=red
inactive_fg=dim grey
bind_to_list=runtime_status
source=decoderFlags
bitval=1
bitmask=8

[runtime_last_period_valid_label]
keys=bind_to_list,source,bitval,bitmask,active_fg,inactive_fg
active_fg=red
inactive_fg=dim grey
bind_to_list=runtime_status
source=decoderFlags
bitval=1
bitmask=16

[runtime_fp_prime_label]
keys=bind_to_list,source,bitval,bitmask,active_fg,inactive_fg
active_fg=red
inactive_fg=dim grey
bind_to_list=runtime_status
source=coreStatusA
bitval=1
bitmask=1

