#----- Rescale Entries ------------------------------------------------------

[rescale_entry]
keys=table_num
table_num=0

#----- Rescale Combos ------------------------------------------------------

[rescale_combo]
keys=table_num,post_functions_with_arg
table_num=0
post_functions_with_arg=lock_entry

#----- MAIN VETable Container ---------------------------------------------

[main_vetable]
keys=rescaler_frame
rescaler_frame=VE16_rescale_frame

#----- Load Entries (MAP/TPS) -----------------------------------------------

[load_entry]
keys=group
offset=58
group=load

#----- RPM Entries ---------------------------------------------------------

[rpm_entry]
keys=group
offset=4
group=rpm

#----- VE-Table Entries -----------------------------------------------------

[ve_entry]
keys=group
offset=100
group=ve
