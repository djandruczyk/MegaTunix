datasource=LoadMain

[gaugeframe3]
keys=post_functions_with_arg,gaugexml,datasource,table_num
table_num=0
post_functions_with_arg=create_gauge
gaugexml=SportComp/AM_SportComp_VE_Gauge.xml
datasource=VEMain

[gaugeframe4]
keys=post_functions_with_arg,gaugexml,datasource,table_num
table_num=0
post_functions_with_arg=create_gauge
gaugexml=SportComp/AM_SportComp_O2Lambda_Gauge.xml
datasource=EGO

[gaugeframe5]
