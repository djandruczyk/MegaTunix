page=0
offset=2
dl_conv_expr=x/100
ul_conv_expr=x*100
raw_lower=1
bind_to_list=spark_controls
dl_type=_IMMEDIATE_
handler=_GENERIC_
tooltip=The soft revlimit point where only spark is retarded to try to limit engien RPM.

[soft_revlimit_sparkangle_entry]
