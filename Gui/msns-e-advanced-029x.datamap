[hard_spark_cut_entry]
keys=page,offset,raw_upper,bind_to_list,dl_type,handler,tooltip
key_types=_INT_,_INT_,_INT_,_STRING_,_ENUM_,_ENUM_,_STRING_
page=0
offset=45
raw_upper=20
bind_to_list=spark_controls,nonedis_controls,limit_spark_ctrls
dl_type=_IMMEDIATE_
handler=_GENERIC_
tooltip=This is the number of sparks to drop for ever number of sparks in the Base sparks variable above,  so if you have a 4 cyle engine set base sparks to 5 and this to one, it'll do an evenly balanced round robin spark drop. Use a higher number for more spark drop and more engien power cutoff.

