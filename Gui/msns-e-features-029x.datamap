bitval=1
bitshift=2
bitmask=247
toggle_groups=wheel_decode
group_states=ENABLED
dl_type=_IMMEDIATE_
handler=_GENERIC_
tooltip=This mode is for general purpose trigger wheels of the -1 or -2 variety (i.e. 60-2, 12-1, 36-1, and so on).

[wheel-1_persona]
keys=page,offset,bitval,bitmask,bitshift,bind_to_list,dl_type,handler,tooltip
key_types=_INT_,_INT_,_INT_,_INT_,_INT_,_STRING_,_ENUM_,_ENUM_,_STRING_
