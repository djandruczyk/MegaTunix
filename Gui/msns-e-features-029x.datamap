key_types=_INT_,_STRING_
page=0
bind_to_list=dt_controls

[inj2_cranking_label]
keys=page,bind_to_list
key_types=_INT_,_STRING_
page=0
bind_to_list=dt_controls

[variable_launch_feature_label]
