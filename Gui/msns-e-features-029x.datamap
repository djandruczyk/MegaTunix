offset=0
bitval=0
bitshift=3
bitmask=8
dl_type=_IMMEDIATE_
handler=_GENERIC_
bind_to_list=wheel_decode
tooltip=This mode is for general trigger wheels following the "x-1" style. Wheels that fall into this class are the ford EDIS 36-1 wheel.  All wheel decoding is done in software. (i.e. no external decoder box needed)

[wheel-2_persona]
keys=page,offset,bitval,bitmask,bitshift,bind_to_list,dl_type,handler,tooltip
key_types=_INT_,_INT_,_INT_,_INT_,_INT_,_STRING_,_ENUM_,_ENUM_,_STRING_
