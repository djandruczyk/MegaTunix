#----- Radio/Checkbuttons ----------------------------------------------------

[traction_enabled_radio_button]
keys=page,offset,bitval,bitmask,bitshift,toggle_groups,group_states,dl_type,handler,tooltip
key_types=_INT_,_INT_,_INT_,_INT_,_INT_,_STRING_,_STRING_,_ENUM_,_ENUM_,_STRING_
page=0
offset=92
bitval=1
bitshift=5
bitmask=32
toggle_groups=global_traction,traction_labels
group_states=ENABLED,ENABLED
dl_type=_IMMEDIATE_
handler=_GENERIC_
tooltip=Enables Traction Control 

[traction_disabled_radio_button]
keys=page,offset,bitval,bitmask,bitshift,dl_type,handler,tooltip
key_types=_INT_,_INT_,_INT_,_INT_,_INT_,_ENUM_,_ENUM_,_STRING_
page=0
offset=92
bitval=0
bitshift=5
bitmask=32
dl_type=_IMMEDIATE_
handler=_GENERIC_
tooltip=Disables Traction Control 

[traction_mode_rpm_radio_button]
keys=page,offset,bitval,bitmask,bitshift,swap_labels,bind_to_list,toggle_groups,group_states,dl_type,handler,tooltip
key_types=_INT_,_INT_,_INT_,_INT_,_INT_,_STRING_,_STRING_,_STRING_,_STRING_,_ENUM_,_ENUM_,_STRING_
page=0
offset=109
bitval=0
bitshift=3
bitmask=8
swap_labels=traction_labels
bind_to_list=global_traction
toggle_groups=vss_traction,rpmdot_traction
group_states=DIABLED,ENABLED
dl_type=_IMMEDIATE_
handler=_GENERIC_
tooltip=Sets the traction mode to be dependant on RPM rate of change (RPMdot). This doesn't require any additional sensors

[traction_mode_vss_radio_button]
keys=page,offset,bitval,bitmask,bitshift,bind_to_list,swap_labels,dl_type,handler,tooltip
key_types=_INT_,_INT_,_INT_,_INT_,_INT_,_STRING_,_STRING_,_ENUM_,_ENUM_,_STRING_
page=0
offset=109
bitval=1
bitshift=3
bitmask=8
swap_labels=traction_labels
bind_to_list=global_traction
dl_type=_IMMEDIATE_
handler=_GENERIC_
tooltip=Sets the traction mode to utilize VSS sensors inputs on Analog input 6 and 7.  This can conflict with second EGO input and EGT input.

[traction_nos_off_override_radio_button]
keys=page,offset,bitval,bitmask,bitshift,bind_to_list,dl_type,handler,tooltip
key_types=_INT_,_INT_,_INT_,_INT_,_INT_,_STRING_,_ENUM_,_ENUM_,_STRING_
page=0
offset=109
bitval=0
bitshift=0
bitmask=1
bind_to_list=global_traction,nos_params
dl_type=_IMMEDIATE_
handler=_GENERIC_
tooltip=Tells the ECU to NOT turn off NOS during traction control (dangerous)

[traction_nos_off_no_override_radio_button]
keys=page,offset,bitval,bitmask,bitshift,bind_to_list,dl_type,handler,tooltip
key_types=_INT_,_INT_,_INT_,_INT_,_INT_,_STRING_,_ENUM_,_ENUM_,_STRING_
page=0
offset=109
bitval=1
bitshift=0
bitmask=1
bind_to_list=global_traction,nos_params
dl_type=_IMMEDIATE_
handler=_GENERIC_
tooltip=Tells the ECU to turn off NOS during traction control (safer)

[traction_rpm_stable_radio_button]
keys=page,offset,bitval,bitmask,bitshift,dl_type,handler,tooltip
key_types=_INT_,_INT_,_INT_,_INT_,_INT_,_ENUM_,_ENUM_,_STRING_
page=0
offset=109
bitval=0
bitshift=2
bitmask=4
dl_type=_IMMEDIATE_
handler=_GENERIC_
tooltip=When this is set traction control retard, enrichment and spark drop will be enabled until the RPM is stable for 0.1 seconds, even if this value is shorter that the number of cycles counts above.

[traction_cycle_count_only_radio_button]
keys=page,offset,bitval,bitmask,bitshift,dl_type,handler,tooltip
key_types=_INT_,_INT_,_INT_,_INT_,_INT_,_ENUM_,_ENUM_,_STRING_
page=0
offset=109
bitval=1
bitshift=2
bitmask=4
dl_type=_IMMEDIATE_
handler=_GENERIC_
tooltip=When this is set traction control retard, enrichment and spark drop will be enabled until the end ofthe cycle counts above ONLY.

